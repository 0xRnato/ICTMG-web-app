{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "extends": [
        "angular"
    ],
    "rules": {
        "no-unused-vars": "warn"
    },
    "globals": {
        "angular": true
    },
    "settings": {
        "angular": 1
    }
}