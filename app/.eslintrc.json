{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "extends": [
        "google"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "require-jsdoc": "warn"
    }
}